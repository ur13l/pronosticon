                                img(src=url('/storage/' +pronostico.partido.equipoVisita.imagen) height="50")
                        else
                            td.center
                                img(src=url('/storage/' +pronostico.partido.equipoVisita.imagen) height="50")
                        

                        td.center
                            if pronostico.partido.resultado[1] != "resultado" 
                                if pronostico.partido.resultado.id_equipo_ganador == pronostico.id_equipo_ganador
                                    img(src=url('/img/success.png'))
                                else
                                    img(src=url('/img/cancel.png'))
                            else
                                img(src=url('/img/wait.png'))

